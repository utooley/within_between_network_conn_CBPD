#declare -a ARRAY=("sub-CBPD0128" "sub-CBPD0136" "sub-CBPD0137" "sub-CBPD0138")
declare -a ARRAY=("sub-CBPD0138")
LENGTH=${#ARRAY[@]}

i=`expr ${SGE_TASK_ID} - 1`

if [[ $i -ge ${LENGTH} ]]; then
 echo 'Array index > than number of elements'

else
 SUB=${ARRAY[$i]}

qsub ${SCRIPTS_DIR}/fmriprep_cmd.sh ${SUB}

fi
